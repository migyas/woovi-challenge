{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
    },
    "allowJs": true,
    "resolveJsonModule": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ],
}